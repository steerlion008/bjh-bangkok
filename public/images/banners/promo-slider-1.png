  display:flex; gap:14px; align-items:center; pointer-events:auto;
  border-radius:9999px;
  padding:12px 14px;            /* กล่องนอกใหญ่ขึ้น */
}
#bjh-contact-bar .bjh-btn{
  display:flex; align-items:center; gap:.65rem; text-decoration:none; color:#fff;
  border-radius:9999px; font-weight:800; line-height:1.1; white-space:nowrap;
  padding:16px 22px;            /* ปุ่มใหญ่ขึ้น */
  font-size:25px;               /* ← ฟอนต์ ~25px */
}
#bjh-contact-bar .bjh-btn svg{ width:24px; height:24px; fill:currentColor; } /* ไอคอนใหญ่ขึ้น */
#bjh-contact-bar .bjh-call{ background:#0ea5e9; }   /* Sky */
#bjh-contact-bar .bjh-chat{ background:#10b981; }   /* Emerald */
#bjh-contact-bar a{ -webkit-tap-highlight-color:transparent; }

/* ---- Very small phones (≤360px): กันล้นเล็กน้อย ---- */
@media (max-width:360px){
  #bjh-contact-bar .bjh-wrap{ padding:10px 12px; gap:10px; }
  #bjh-contact-bar .bjh-btn{ padding:14px 18px; font-size:22px; }
  #bjh-contact-bar .bjh-btn svg{ width:22px; height:22px; }
}

/* ---- Tablet / Notebook (≥768px): เพิ่มช่องไฟให้โปร่งตา ---- */
@media (min-width:768px){
  #bjh-contact-bar .bjh-wrap{ padding:14px 16px; gap:16px; }
  #bjh-contact-bar .bjh-btn{ padding:18px 24px; }
}

/* ---- Desktop (≥1200px): ยังคงขนาดใหญ่ อ่านง่าย ---- */
@media (min-width:1200px){
  #bjh-contact-bar{ bottom:calc(env(safe-area-inset-bottom) + 20px); }
  #bjh-contact-bar .bjh-wrap{ padding:14px 18px; gap:18px; }
}
