<script>
(function(){
  // ----- Load Pixel once -----
  if (!window.fbq) {
    !function(f,b,e,v,n,t,s){
      if(f.fbq) return; n=f.fbq=function(){ n.callMethod ?
        n.callMethod.apply(n,arguments) : n.queue.push(arguments) };
      if(!f._fbq) f._fbq=n; n.push=n; n.loaded=!0; n.version='2.0';
      n.queue=[]; t=b.createElement(e); t.async=!0; t.src=v;
      s=b.getElementsByTagName(e)[0]; s.parentNode.insertBefore(t,s);
    }(window, document, 'script', 'https://connect.facebook.net/en_US/fbevents.js');

    fbq('init', '1142015364237432');          // ← Pixel ID BJH
    fbq('track', 'PageView');
  }

  // ----- Helpers -----
  const BJH = window.BJH || (window.BJH = {});
  BJH.flags = BJH.flags || {};                       // กันยิงซ้ำ
  BJH.mkId  = () => 'BJH-' + Math.floor(Date.now()/1000) + '-' +
                     Math.random().toString(36).slice(2,8);

  function trackOnce(key, fn){
    if (BJH.flags[key]) return; BJH.flags[key] = 1; try{ fn(); }catch(e){}
  }

  // ----- 1) Lead on Thank-You -----
  (function(){
    const isTYPath  = /\/(thank[-_]?you|booking-success|success|completed|ขอบคุณ|สำเร็จ)\/?/i
                      .test(location.pathname);
    const isTYQuery = /[?&](success|completed|thankyou|ty)=1/i.test(location.search);
    if (!(isTYPath || isTYQuery)) return;

    trackOnce('lead_on_ty', function(){
      const eid = BJH.mkId();
      fbq('track', 'Lead', {}, { eventID: eid });
      window._bjh_event_id = eid;
      if (window.console) console.log('[BJH] Pixel Lead fired with eventID:', eid);
    });
  })();

  // ----- 2) ViewContent on Homepage -----
  (function(){
    if (location.pathname === '/') {
      trackOnce('vc_home', function(){
        fbq('track', 'ViewContent', { content_category: 'homepage' });
      });
    }
  })();

  // ----- 3) Global click tracking (call/chat) -----
  // จับทุกลิงก์ tel: และแชทยอดนิยม แม้จะไม่ใช่ปุ่มลอยก็ตาม
  (function(){
    function providerFromURL(href){
      try{
        const u = new URL(href);
        if (/m\.me|facebook\.com\/messages/i.test(u.hostname)) return 'messenger';
        if (/wa\.me|api\.whatsapp\.com/i.test(u.hostname))     return 'whatsapp';
        if (/line\.me/i.test(u.hostname))                      return 'line';
      }catch(e){}
      return 'other';
    }
    document.addEventListener('click', function(ev){
      const a = ev.target.closest && ev.target.closest('a');
      if (!a || !a.href) return;

      // โทรศัพท์
      if (a.href.startsWith('tel:')) {
        trackOnce('call_' + a.href, function(){
          const eid = BJH.mkId();
          fbq('track', 'Contact', { method: 'call', href: a.href }, { eventID: eid });
          fbq('trackCustom', 'CallClick', { href: a.href, page: location.pathname });
        });
        return;
      }

      // แชทยอดนิยม
      if (/(m\.me|facebook\.com\/messages|wa\.me|api\.whatsapp\.com|line\.me)/i.test(a.href)) {
        const prov = providerFromURL(a.href);
        trackOnce('chat_' + a.href, function(){
          const eid = BJH.mkId();
          fbq('track', 'Contact', { method: 'chat', provider: prov, href: a.href }, { eventID: eid });
          fbq('trackCustom', 'ChatClick', { provider: prov, href: a.href, page: location.pathname });
        });
      }
    }, true);
  })();
})();
</script>

