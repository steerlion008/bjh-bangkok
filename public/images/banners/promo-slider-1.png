<script>
(function(){
  const bar = document.getElementById('bjh-contact-bar');
  if (!bar) return;
  if (bar.parentElement !== document.body) document.body.appendChild(bar);

  const phone = (bar.dataset.phone || '').replace(/\s|-/g,'');
  const chat  = bar.dataset.chat  || '';

  const callBtn = document.getElementById('bjh-call-btn');
  const chatBtn = document.getElementById('bjh-chat-btn');
  if (phone) callBtn.href = `tel:${phone}`;

  // UTM for analytics
  const utm = new URLSearchParams({
    utm_source:'site', utm_medium:'floating_bar',
    utm_campaign:'contact_quick', utm_content:location.pathname
  }).toString();

  if (chat) {
    try {
      const u = new URL(chat);
      if (/wa\.me|api\.whatsapp\.com/.test(u.hostname)) {
        u.searchParams.set('text', `Hi BJH, from ${location.href}`);
        u.searchParams.set('utm_source','site');
        u.searchParams.set('utm_medium','floating_bar');
        u.searchParams.set('utm_campaign','contact_quick');
        chatBtn.href = u.toString();
      } else if (/m\.me|facebook\.com/.test(u.hostname)) {
        u.searchParams.set('ref', utm);
        chatBtn.href = u.toString();
      } else if (/line\.me/.test(u.hostname)) {
        chatBtn.href = chat + (chat.includes('#') ? '&' : '#') + utm;
      } else {
        chatBtn.href = chat + (chat.includes('?')?'&':'?') + utm;
      }
    } catch(e){ chatBtn.href = chat; }
  } else { chatBtn.style.display = 'none'; }

  // กันชน cookie/popups ด้านล่าง
  function adjustBottom(){
    const cookie = document.querySelector('[id*="cookie"], .cookie-banner, .cky-consent-container, .nitropack-cookie-container');
    const r = cookie?.getBoundingClientRect();
    const overlap = r && r.height && r.top < innerHeight && r.bottom > innerHeight - 110;
    bar.style.bottom = overlap ? `calc(${(r?.height||0)+20}px + env(safe-area-inset-bottom))`
                               : 'calc(env(safe-area-inset-bottom) + 16px)';
  }
  const ro = new ResizeObserver(adjustBottom);
  ro.observe(document.body);
  addEventListener('scroll', adjustBottom, {passive:true});
  adjustBottom();

  // GA4/GTM event (คงเดิมจากเวอร์ชันก่อน)
  function track(action,label){
    window.dataLayer = window.dataLayer || [];
    window.dataLayer.push({event:'bjh_contact_click',action,label,page:location.pathname,ts:Date.now()});
  }
  callBtn.addEventListener('click', ()=>track('call', phone));
  chatBtn.addEventListener('click', ()=>track('chat', chatBtn.href));
})();
</script>
